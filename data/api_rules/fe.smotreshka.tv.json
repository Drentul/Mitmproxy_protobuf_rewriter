{
    "server": ["fe.smotreshka.tv", "fe.rc.smotreshka.tv"],
    "errors": 
    [
        {"proto_message":"HttpFormErrors", "module":"general_pb2"},
        {"proto_message":"HttpError", "module":"general_pb2"}
    ],
    "rules":
    [
        {
            "path":"/static/.*.json",
            "method":"GET",
            "proto_message":"text"
        },
        {
            "path":"vod/v2/",
            "method":"GET",
            "proto_message":"VODCollection",
            "module":"vod.v2.vod_pb2"
        },
        {
            "path":"/auth/v1/with-verified-contacts/available-logins",
            "method":"GET",
            "proto_message":"AvailableLogins",
            "module":"auth.v1.available_logins_pb2"
        },
        {
            "path":"/app-info",
            "method":"GET",
            "proto_message":"AppInfoV2",
            "module":"general_pb2"
        },
        {
            "path":"/v2/account",
            "method":"GET",
            "proto_message":"Account",
            "module":"accounts_pb2"
        },
        {
            "path":"/login",
            "method":"POST",
            "proto_message":"User",
            "module":"user_pb2"
        },
        {
            "path":"/login/qr",
            "method":"GET",
            "proto_message":"User",
            "module":"user_pb2"
        },
        {
            "path":"/logout",
            "method":"GET",
            "proto_message":"Message",
            "module":"message_pb2"
        },
        {
            "path":"/account/change-profile",
            "method":"GET",
            "proto_message":"Message",
            "module":"message_pb2"
        },
        {
            "path":"/channels",
            "method":"GET",
            "proto_message":"EPG",
            "module":"epg_pb2"
        },
        {
            "path":"/walls/0",
            "method":"GET",
            "proto_message":"MainWall",
            "module":"recommendations_pb2"
        },
        {
            "path":"/walls/1",
            "method":"GET",
            "proto_message":"PersonalContentWall",
            "module":"recommendations_pb2"
        },
        {
            "path":"/purchase-info",
            "method":"GET",
            "proto_message":"PurchaseInfos",
            "module":"purchases_pb2"
        },
        {
            "path":"/playback-info/[^/]*",
            "method":"GET",
            "proto_message":"LivePlaybackInfo",
            "module":"playback_pb2"
        },
        {
            "path":"/channels/[^/]*/programs",
            "method":"GET",
            "proto_message":"EPG",
            "module":"epg_pb2"
        },
        {
            "path":"/pauses/[^/]*",
            "method":"GET",
            "proto_message":"ChannelPauses",
            "module":"playback_pb2"
        },
        {
            "path":"/v2/settings/profile/restrictions",
            "method":"GET",
            "proto_message":"ProfileRestrictions",
            "module":"accounts_pb2"
        }
        ,
        {
            "path":"/vod/v2/archive/titles/[^/]*/episodes",
            "method":"GET",
            "proto_message":"VODEpisodes",
            "module":"vod.v2.vod_pb2"
        },
        {
            "path":"/vod/v2/archive/titles/[^/]*",
            "method":"GET",
            "proto_message":"VODTitle",
            "module":"vod.v2.vod_pb2"
        },
        {
            "path":"/pause/vod/v1/titles/archive/[^/]*",
            "method":"GET",
            "proto_message":"VODPauses",
            "module":"pauses.v1.pauses_pb2"
        }
    ]
}
